<a href="/new">Enter a new budget</a>

<table>
  <tr>
    <th>Item Name</th>
    <th>Amount</th>
    <th>Month</th>
  </tr>
  <% sort_by_month(@budgets).each do |budget| %>
    <tr>
      <td><%= budget["Item Name"] %></td>
      <td><%= parse_currency(budget["Amount"]) %></td>
      <td><%= budget["Month"] %></td>
    </tr>
  <% end %>
</table>